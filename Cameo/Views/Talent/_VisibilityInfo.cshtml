@model IEnumerable<string>

@if (Model.Count() > 0)
{
    <!-- Start Icon Box -->
    <div class="alert alert-warning">
        <div class="funfact-area ptb_md--80 ptb_sm--60 bg-color">
            <div class="container">
                <div class="row">
                    <h4>В данный момент Вы <b>не видны в поиске</b>. Пожалуйста, заполните следующие данные:</h4>
                    <ul class="list-container">
                        @foreach (var item in Model)
                        {
                            <li class="list-items">
                                <span class="list__icon"><i class="fa fa-times"></i></span>
                                <span class="list__text">@item</span>
                            </li>
                        }
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- End Icon Box -->
}