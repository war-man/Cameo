<style>
    /*a.button-link.button__black,*/
    a.button-link,
    button.button-link.button__black,
    input[type=submit],
    input[type=text],
    textarea,
    .blog.blog_style--1,
    #homeHeaderVideo {
        border-radius: 0.25rem !important;
    }

    .input-group .input-group-prepend {
        border-bottom-left-radius: 0.25rem !important;
        border-top-left-radius: 0.25rem !important;
    }
    .input-group > input[type=text] {
        border-bottom-right-radius: 0.25rem !important;
        border-top-right-radius: 0.25rem !important;
        border-bottom-left-radius: 0rem !important;
        border-top-left-radius: 0rem !important;
    }

    ::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */
        color: #d3d3d3;
        opacity: 1; /* Firefox */
    }

    :-ms-input-placeholder { /* Internet Explorer 10-11 */
        color: #d3d3d3;
    }

    ::-ms-input-placeholder { /* Microsoft Edge */
        color: #d3d3d3;
    }

    .backBtn, .submitBtn {
        width: 100%;
        text-transform: uppercase;
        font-weight: 700;
        font-size: 14px;
    }

    input[disabled=disabled],
    button[disabled=disabled] {
        color: #a6a3a3 !important;
        cursor: not-allowed;
    }

    .singleTalentImg {
        object-fit: cover; /* Scale the image so it covers whole area, thus will likely crop */
        object-position: center; /* Center the image within the element */
        height: 300px;
        width: 100%;
        background: linear-gradient(90deg, #dddcdb, #d9d9d9, #ccc, #bdbdbd);
        background-size: 400% 400%;
        animation: gradient 15s ease infinite;
    }

    @@keyframes gradient {
	    0% {
		    background-position: 0% 50%;
	    }
	    50% {
		    background-position: 100% 50%;
	    }
	    100% {
		    background-position: 0% 50%;
	    }
    }

    .team__style--4 .thumbnail2 {
        border-radius: 0.25rem;
        position: relative;
        overflow: hidden;
        margin: 0 auto;
        cursor: pointer;
    }
    .team .thumbnail2 .team-badge {
        position: absolute;
        /*z-index: 9;*/
        top: 3px;
        left: auto;
        right: 5px;
        color: white;
        font-size: 11px;
        font-weight: 500;
        background: rgba(0,0,0,0.3);
        padding: 0px 3px;
        border-radius: 5px;
    }

    .team__style--4 .thumbnail3 {
        border-radius: 0.25rem;
        position: relative;
        overflow: hidden;
        margin: 0 auto;
        cursor: pointer;
    }
    .singleTalentInVideoRequestImg {
        object-fit: cover; /* Scale the image so it covers whole area, thus will likely crop */
        object-position: center; /* Center the image within the element */
        height: 90px;
        width: 100%;
        /*background: #5e35b1;*/
        background: linear-gradient(-45deg, #ee7752, #e73c7e, #23a6d5, #23d5ab);
        background-size: 400% 400%;
        animation: gradient 15s ease infinite;
    }

    /*.tab-vertical-wrapper .tab_style--3 .nav-item .nav-link.active {
        color: #5e35b1;
    }*/

    .videoBox {
        position: relative;
    }
    .videoBox .contolActionBtn {
        position: absolute;
        /*z-index: 9;*/
        top: 80%;
        left: 10%;
        right: auto;
        color: white;
        font-size: 23px;
        font-weight: 500;
        background: rgba(0,0,0,0.3);
        padding: 0px 11px;
        border-radius: 30px;
        cursor: pointer;
        width: 41px;
    }

    video.requestVideo {
        height: 400px;
        object-fit: cover;
        object-position: center;
    }

    .innerContainer
    {
        width: 100%;
    }
</style>