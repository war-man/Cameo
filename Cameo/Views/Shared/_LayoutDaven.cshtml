    <script src="~/lib/DavenTheme/js/revolution.extension.slideanims.min.js"></script>
    <script src="~/lib/DavenTheme/js/revolution.extension.video.min.js"></script>
    <script src="~/lib/DavenTheme/js/revoulation.js"></script>

    <partial name="_FirebaseConfig" />

