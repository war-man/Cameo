</head>
<body class="template-color-3 template-font-1"><!--template-color-3,4-->
    @*<partial name="_CookieConsentPartial" />*@

    <!-- Wrapper -->
    <div id="wrapper" class="wrapper" style="background-color: rgb(249, 249, 249);">
        <!-- Start Header -->
        <partial name="_HeaderDaven" />
        <!-- Start Header -->
        <!-- Start Loader Area -->
        @*<partial name="_LoaderAreaDaven" />*@
        <!-- End Loader Area -->
        <!-- Start Page Wrapper -->
        <main class="page-content">
            @RenderBody()
        </main>
        <!-- End Page Wrapper -->
        <!-- Start Footer Area -->
        <partial name="_FooterDaven" />
        <!-- End Footer Area -->
    </div>
    <!--// Wrapper -->

    <script src="~/lib/DavenTheme/js/ajax-mail.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/DavenTheme/js/plugins.js"></script>
    <script src="~/lib/DavenTheme/js/main.js"></script>

